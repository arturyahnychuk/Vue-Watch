{
  "name": "@dangvanthanh/vue-clock",
  "version": "0.1.7",
  "description": "Simple clock using Vue.js",
  "author": "Dang Van Thanh <dangvanthanh@dangthanh.org>",
  "main": "dist/vue-clock.umd.js",
  "module": "dist/vue-clock.esm.js",
  "unpkg": "dist/vue-clock.min.js",
  "style": "vue-clock/dist/vue-clock.esm.css",
  "scripts": {
    "build": "npm run build:browsers && npm run build:es && npm run build:umd",
    "build:browsers": "rollup -c build/rollup.config.browsers.js",
    "build:es": "rollup -c build/rollup.config.es.js",
    "build:umd": "rollup -c build/rollup.config.umd.js"
  },
  "keywords": [
    "vue",
    "vue-clock",
    "clock"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "clean-css": "^4.2.3",
    "esbuild": "^0.6.30",
    "rollup": "^2.26.10",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-esbuild": "^2.5.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue-template-compiler": "^2.6.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dangvanthanh/vue-clock.git"
  },
  "bugs": {
    "url": "https://github.com/dangvanthanh/vue-clock/issues"
  },
  "homepage": "https://github.com/dangvanthanh/vue-clock#readme"
}
